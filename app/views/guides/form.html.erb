<%= render 'nabvar' %>

<div class="container">
  <div class="registration-form">
    <form>
      <div class="form-icon">
        <span><i class="fa-solid fa-user"></i></span>
      </div>
      <div class="form-group">
        <input type="text" class="form-control item" id="username" placeholder="Nombre completo">
      </div>
      <div class="form-group">
        <input type="text" class="form-control item" id="email" placeholder="Email">
      </div>
      <div class="form-group">
        <input type="password" class="form-control item" id="password" placeholder="Contraseña">
      </div>

      <div class="form-group">
        <input type="text" class="form-control item" id="phone-number" placeholder="Numero de telefono">
      </div>
      <div class="form-group">
        <textarea type="text-area" class="form-control item" id="about" placeholder="Cuentanos más acerca de ti"></textarea>
      </div>
      <div class="form-group">
        <button type="button" class="btn btn-block create-account">Enviar solicitud</button>
      </div>
    </form>
  </div>
</div>