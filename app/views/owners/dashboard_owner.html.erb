          <button class="cell-more-button">
            <img src="https://i.ibb.co/DCmc2F1/profile.png" style="width: 18px; height: 18px">
          </button>
          <div class="product-cell image">
            <img src="https://i.ibb.co/DCmc2F1/profile.png" style="width: 48px; height: 38px">
            <!--          hacer dinamico Nombre completo-->
            <span>
             <%= @user[:names].sample %>
            </span>
          </div>
          <!--          hacer dinamico Rol-->
          <div class="product-cell category"><span class="cell-label">Rol:</span>
            <%= @user[:rol].sample %>
          </div>
          <div class="product-cell status-cell">
            <span class="cell-label">Estado:</span>
            <!--          hacer dinamico Estado-->
            <button class="status active btn">
              <%= @user[:status].sample %>
            </button>
          </div>
          <!--            hacer dinamico Correo electronico-->
          <div class="product-cell sales"><span class="cell-label">Correo electronico:</span>
            <%= @user[:email].first %>
          </div>
          <!--            hacer dinamico Viajes ofrecidos -->
          <div class="product-cell stock"><span class="cell-label">Viajes ofrecidos:</span>
            <%= @user[:travel_ofert].sample %>
          </div>
          <!--            hacer dinamico Viajes contratados -->
          <div class="product-cell price"><span class="cell-label">Viajes contratados:</span>
            <%= @user[:travel_contrac].sample %>
          </div>
        </div>
